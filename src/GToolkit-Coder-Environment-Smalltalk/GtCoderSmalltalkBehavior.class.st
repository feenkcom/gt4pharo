	#superclass : #GtCoderBaseSmalltalkBehavior,
	#instVars : [
		'environment'
	],
	#category : #'GToolkit-Coder-Environment-Smalltalk'
}

{ #category : #accessing }
GtCoderSmalltalkBehavior >> environment [

