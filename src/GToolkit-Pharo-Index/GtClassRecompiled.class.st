Class {
	#name : #GtClassRecompiled,
	#superclass : #ClassAnnouncement,
	#instVars : [
		'classAffected'
	],
	#category : #'GToolkit-Pharo-Index'
}

{ #category : #accessing }
GtClassRecompiled >> classAffected [
	^ classAffected
]

{ #category : #accessing }
GtClassRecompiled >> classAffected: aClass [
	classAffected := aClass
]
