Class {
	#name : #GtPharoSuiteSelectedStateChanged,
	#superclass : #Announcement,
	#instVars : [
		'state'
	],
	#category : #'GToolkit-Pharo-TestSuite-UI-Events'
}

{ #category : #accessing }
GtPharoSuiteSelectedStateChanged >> state [

	^ state
]

{ #category : #accessing }
GtPharoSuiteSelectedStateChanged >> state: anObject [

	state := anObject
]
